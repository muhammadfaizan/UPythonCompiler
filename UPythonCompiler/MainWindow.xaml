        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="450" Width="625">
    <Grid >
        <TextBox Name="Source" HorizontalAlignment="Left" Height="372" Margin="10,37,0,0" TextWrapping="Wrap" Text="Source Text Box" VerticalAlignment="Top" Width="287" TextChanged="Source_TextChanged"/>
        <TextBox Name="Tokens" HorizontalAlignment="Right" Height="372" Margin="0,37,10,0" TextWrapping="Wrap" Text="LA Text Box" VerticalAlignment="Top" Width="292"/>
        <GridSplitter HorizontalAlignment="Left" Height="372" Margin="305,37,0,0" VerticalAlignment="Top" Width="5"/>
        <Button Content="Compile" HorizontalAlignment="Left" Margin="531,12,0,0" VerticalAlignment="Top" Width="76" Click="Button_Click"/>

    </Grid>
</Window>
